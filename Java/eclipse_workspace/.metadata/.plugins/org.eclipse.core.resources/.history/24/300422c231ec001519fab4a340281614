
public class Name {
	void Name(String vorname, String nachname) {
		this.vorname = vorname;
		this.nachname = nachname;
	}

	String vorname = "Leonard";
	String nachname = "Oertelt";

	String getVollerName() {
		return vorname + " " + nachname;
	}

	String getVollerNameRAK() {
		return nachname + ", " + vorname;
	}
}
