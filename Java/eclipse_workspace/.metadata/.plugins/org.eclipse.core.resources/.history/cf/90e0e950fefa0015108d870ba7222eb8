import java.io.File;
import java.util.Map;
import java.util.TreeMap;
import java.util.TreeSet;

public class IndexManager {

	private final String pathToIndexFile;
	private Map<String, TreeSet<File>> indexFromFile = new TreeMap<String, TreeSet<File>>();

	public String getPathToIndexFile() {
		return pathToIndexFile;
	}

	public Map<String, TreeSet<File>> getFoundFiles() {
		return indexFromFile;
	}

	IndexManager(String pathToIndex) {
		this.pathToIndexFile = pathToIndex;
	}

	boolean loadIndex() {
		return false;
	}

}
