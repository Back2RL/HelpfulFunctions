#Muster Makefile

#$<	die erste Abh채ngigkeit
#$@	Name des targets
#$+	eine Liste aller Abh채ngigkeiten
#$^	eine Liste aller Abh채ngigkeiten, wobei allerdings doppelt vorkommende Abh채ngigkeiten eliminiert wurden.

GCC_ARGS = -Wall -ansi -pedantic-errors -Wstrict-prototypes
OBJ = main.o 
NAME = Main

$(NAME): $(OBJ)
	gcc $(GCC_ARGS) -o $@ $(OBJ)

%.o: %.c
	gcc $(GCC_ARGS) -c $<


clean:
	rm $(NAME) $(OBJ)
